{
    "name": "@eyra/feldspar",
    "version": "0.1.0",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist"
    ],
    "scripts": {
        "build:css": "NODE_ENV=production tailwindcss build -i ./src/framework/styles.css -o ./dist/styles.css --minify",
        "build:wheel": "cd src/framework/processing/py && poetry build --format wheel",
        "build:install-wheel": "cp -R src/framework/processing/py/dist/. ./dist",
        "build:py": "npm run build:wheel && npm run build:install-wheel",
        "build:ts": "rollup -c",
        "build": "npm run build:ts && npm run build:py && npm run build:css",
        "start:py": "nodemon --ext py --exec \"npm run build:py\"",
        "dev": "rollup -c -w",
        "test": "jest"
    },
    "peerDependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "dependencies": {
        "@lottiefiles/dotlottie-react": "^0.12.1",
        "@types/lodash": "^4.17.14",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.2",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-typescript": "^12.1.2",
        "@rollup/plugin-url": "^8.0.2",
        "@types/react": "^19.0.7",
        "@types/react-dom": "^19.0.3",
        "rollup": "^4.30.1",
        "rollup-plugin-dts": "^6.1.1",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-terser": "^7.0.2",
        "tslib": "^2.8.1"
    }
}